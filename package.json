{
  "name": "angular-movie-search",
  "description": "Learn how to create a movie search app with Babel, Browserify, and Angular 1.5",
  "version": "0.4.0",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false,
          "comments": true
        }
      ]
    ]
  },
  "scripts": {
    "start": "gulp watch",
    "test": "gulp test"
  },
  "dependencies": {
    "angular": "^1.5.0-beta",
    "angular-local-storage": "^0.2.2",
    "angular-sanitize": "^1.5.0-beta",
    "angular-ui-router": "^0.2.15"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0-beta",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.9.4",
    "browserify": "^12.0.1",
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-jshint": "^2.0.0",
    "gulp-karma": "0.0.5",
    "gulp-minify-css": "^1.2.1",
    "gulp-minify-html": "^1.0.4",
    "gulp-ng-annotate": "^1.1.0",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.4.1",
    "gulp-usemin": "^0.3.14",
    "jasmine-core": "^2.3.4",
    "jshint-stylish": "^2.0.1",
    "karma": "^0.13.10",
    "karma-browserify": "^4.3.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
